

#include <iostream>

using namespace std;

int main()
{
    int a,b,rev=0;
    cout<<"enter a number";
    cin>>a;
    while(a!=0)
    { b=a%10;
      rev=(rev*10)+b;
      a=a/10;}
     
     while(rev>0) 
    {  
      switch(rev%10){
          
          case 0:
          cout<<"zero  ";
          break;
          
          case 1:
          cout<<"one  ";
          break;
          
          case 2:
          cout<<"two  ";
          break;

          case 3:
          cout<<"three  ";
          break;

          case 4:
          cout<<"four  ";
          break;
          
          case 5:
          cout<<"five  ";
          break;
          
          case 6:
          cout<<"six  ";
          break;
          
          case 7:
          cout<<"seven  ";
          break;
          
          case 8:
          cout<<"eight  ";
          break;
          
          case 9:
          cout<<"nine  ";
          break;
          
      }rev=rev/10;
        
    }
    return 0;
}
